{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "*/10 * * * *"
    }
  ],
  "env": {
    "REVENUECAT_API_KEY": "@revenuecat_api_key",
    "OPENROUTER_API_KEY": "@openrouter_api_key",
    "EXPO_PUBLIC_ORIGIN_MOBILE": "@expo_public_origin_mobile",
    "CRON_SECRET": "@cron_secret"
  }
}
